{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
        "jsx": "react",
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ]
        }
    },
    "include": [
        "src/**/*",
        "index.js",
        "App.js",
        "index.ts",
        "App.ts",
        "index.tsx",
        "App.tsx"
    ]
}