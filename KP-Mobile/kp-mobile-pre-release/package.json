{
  "name": "KelasPintar-production",
  "version": "5.3.12",
  "private": true,
  "scripts": {
    "git-info": "node src/gitInfo.js",
    "ios": "ENVFILE=.env.production && react-native run-ios --simulator='iPhone 14 Pro'",
    "ios:dev": "ENVFILE=.env.development && react-native run-ios --simulator='iPhone 14 Pro'",
    "android": "react-native run-android --mode=productionDebug --active-arch-only",
    "android:dev": "react-native run-android --mode=developmentDebug --appIdSuffix=dev --active-arch-only",
    "android:stag": "react-native run-android --mode=stagingDebug --appIdSuffix=stag --active-arch-only",
    "build:android:dev": "cd android && ./gradlew clean &&  ./gradlew assembleDevelopmentRelease && cd ../",
    "build:android:stag": "cd android && ./gradlew clean && ./gradlew assembleStagingRelease && cd ../",
    "build:android:release": "cd android && ./gradlew clean && ./gradlew assembleProductionRelease && cd ../",
    "build:android:dev:debug": "cd android && ./gradlew clean &&  ./gradlew assembleDevelopmentDebug && cd ../",
    "build:android:stag:debug": "cd android && ./gradlew clean &&  ./gradlew assembleStagingDebug && cd ../",
    "build:android:production:debug": "cd android && ./gradlew clean && ./gradlew assembleProductionDebug && cd ../",
    "bundle:android:dev": "cd android && ./gradlew clean && ./gradlew bundleDevelopmentRelease && open ./app/build/outputs/bundle && cd ../",
    "bundle:android:stag": "cd android && ./gradlew clean && ./gradlew bundleStagingRelease && open ./app/build/outputs/bundle && cd ../",
    "bundle:android:release": "cd android && ./gradlew clean && ./gradlew bundleProductionRelease",
    "lint": "eslint . --quiet",
    "start": "react-native start",
    "start:dev": "ENVFILE=.env.development react-native start",
    "start:stag": "ENVFILE=.env.staging react-native start",
    "test": "jest",
    "reset": "npm start -- --reset-cache",
    "copy:keystore": "cp keystore_production ./android/keystore_production.keystore && cp keystore_production ./android/app/keystore_production.keystore",
    "postinstall": "npm run git-info && npm run copy:keystore && patch-package && react-native setup-ios-permissions && npx pod-install && npm run build:ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "prepare": "husky"
  },
